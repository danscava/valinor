[ITEMDEF i_memory_target_lock]
ID=i_memory
TYPE=t_eq_script
NAME=lock target

ON=@Create
	ATTR=attr_decay|attr_invis
	TIMER=15

ON=@Equip
	TARGET hangi e$ya sabitlenecekti?
	RETURN 1

ON=@Targon_Item
	IF ((<CONT.TARG.ATTR> & attr_move_never) || (<CONT.TARG.ATTR> & attr_static))
		CONT.TARG.MESSAGE Sabitligi kaldirildi.
		CONT.TARG.ATTR=<CONT.TARG.ATTR>&~attr_move_never
		CONT.TARG.ATTR=<CONT.TARG.ATTR>&~attr_static
		CONT.TARG.LINK=04fffffff
	ELSE
		CONT.TARG.MESSAGE Sabitlendi.
		CONT.TARG.ATTR=<CONT.TARG.ATTR>|attr_move_never
		CONT.TARG.LINK=<CONT.UID>
	ENDIF

	TRIGGER=@Timer
	RETURN 1

ON=@Targon_Char
	CONT.SYSMESSAGE elemanlari sabitleyemessin, onlar sabit zaten bisi olmaz onlara korkma.
	TRIGGER=@Timer
	RETURN 1

ON=@Timer
	REMOVE
	RETURN 1

