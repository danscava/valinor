[function sa_check]
if (<account.plevel> > 1) && (<account.plevel> < 7)
	src.try var.sa_count (<var.sa_count> + 1)
	src.try var.sa_uid<eval <var.sa_count>> <uid>
	src.try var.sa_slot<eval <var.sa_count>> <uid.<var.sa_uid<eval <var.sa_count>>>.account.name>, cnct: <uid.<var.sa_uid<eval <var.sa_count>>>.cnct>m
	src.try var.sa_ip<eval <var.sa_count>> adr: <account.lastip>
	src.try var.sa_p<eval <var.sa_count>> <p>
	if (<account.plevel> == 2)
		src.try var.sa_name<eval <var.sa_count>> trg: Counselor <name>
	elseif (<account.plevel> == 3)
		src.try var.sa_name<eval <var.sa_count>> trg: Seer <name>
	elseif (<account.plevel> == 4)
		src.try var.sa_name<eval <var.sa_count>> trg: Gm <name>
	elseif (<account.plevel> == 5)
		src.try var.sa_name<eval <var.sa_count>> trg: Senior Gm <name>
	elseif (<account.plevel> > 5)
		src.try var.sa_name<eval <var.sa_count>> trg: Admin <name>
	endif
	src.try var.sa_mchk<eval <var.sa_count>> 4880
endif

